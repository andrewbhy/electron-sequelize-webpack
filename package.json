{
  "name": "electron-app",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.5",
    "electron": "^1.6.11",
    "flow": "^0.2.3",
    "flow-bin": "^0.50.0",
    "rimraf": "^2.6.1",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "dependencies": {
    "autoprefixer": "7.1.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-preset-react-app": "^3.0.1",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.5",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.34.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "lokijs": "^1.5.0",
    "node-loader": "^0.6.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.6",
    "promise": "7.1.1",
    "react": "^15.5.4",
    "react-dev-utils": "^3.0.2",
    "react-dom": "^15.5.4",
    "react-error-overlay": "^1.0.9",
    "react-hammerjs": "^0.5.0",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "next",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sanitize.css": "^5.0.0",
    "sequelize": "^4.7.5",
    "sequelize-msnodesqlv8": "^0.2.6-beta.5",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.3",
    "tedious": "^2.0.0",
    "url-loader": "0.5.9",
    "uuid": "^3.1.0",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.5.0",
    "webpack-manifest-plugin": "1.1.0",
    "webpack2-externals-plugin": "^1.0.0",
    "whatwg-fetch": "2.0.3"
  },
  "main": "src/electron/main.js",
  "scripts": {
    "dev-app": "node scripts/start.js",
    "dev-electron": "electron -r babel-register .",
    "dev": "cross-env DEV=true NODE_ENV=development concurrently \"yarn run dev-app\" \"yarn run dev-electron\"",
    "clean-modules": "rimraf node_modules",
    "clean": "rimraf build",
    "start": "yarn run clean & yarn run build & yarn run build-electron & electron build\\main-electron.js",
    "start-debug": "yarn run build-electron & electron build\\main-electron.js",
    "build": "node scripts/build.js",
    "build-electron": "webpack --config config\\webpack.config.prod.electron.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
